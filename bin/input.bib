author={Tiago,Matheus N. and Ginja,Gabriel A. and Gounella,Rodrigo and Fonoff,Erich T. and Colombari,Eduardo and Melkzedekue M Alcântara Moreira and Afonso,Jose A. and Monteiro,Vitor and Afonso,Joao L. and Carmo,João P.},
year={2023},
title={Wireless Device with Energy Management for Closed-Loop Deep Brain Stimulation (CLDBS)},
journal={Electronics},
