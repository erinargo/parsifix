author={Pereira,Carlos and Mesquita,João and Guimarães,Diana and Santos,Frederico and Almeida,Luis and Aguiar,Ana},
year={2019},
title={Open IoT Architecture for Continuous Patient Monitoring in Emergency Wards},
journal={Electronics},
volume={8},
