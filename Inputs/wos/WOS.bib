@article{Gay2015,
 title        = {Le Bon Samaritain: A Community-Based Care Model Supported by Technology.},
 author       = {Gay, Valerie and Leijdekkers, Peter and Gill, Asif and Felix Navarro, Karla},
 year         = 2015,
 month        = 2015,
 day          = {07-30},
 journal      = {Studies in health technology and informatics},
 volume       = 214,
